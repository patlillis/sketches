{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "es2019"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "typeRoots": ["types", "node_modules/@types"],
    "baseUrl": ".",
    "paths": { "*": ["*", "types/*"] }
  }
}
